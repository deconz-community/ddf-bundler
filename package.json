{
  "name": "vite-vanilla-ts-lib-starter",
  "version": "0.0.2",
  "private": true,
  "exports": {
    ".": {
      "require": "./dist/vite-ts-lib-starter.cjs",
      "import": "./dist/vite-ts-lib-starter.mjs"
    }
  },
  "main": "./dist/vite-ts-lib-starter.cjs",
  "module": "./dist/vite-ts-lib-starter.mjs",
  "scripts": {
    "dev": "vite --host --port 4000",
    "build": "vite build && dts-bundle-generator --config ./dts-bundle-generator.config.ts",
    "build:dts": "dts-bundle-generator --config ./dts-bundle-generator.config.ts",
    "build:vite": "vite build",
    "test": "vitest",
    "lint": "eslint .",
    "lint:scripts": "eslint . --ext .ts",
    "lint:styles": "stylelint ./**/*.{css,scss}",
    "format:scripts": "prettier . --write",
    "format:styles": "stylelint ./**/*.{css,scss} --fix",
    "format": "npm run format:scripts && npm run format:styles",
    "up": "taze major -I"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.2",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@volar-plugins/prettier": "^1.1.6",
    "@volar-plugins/prettyhtml": "^1.0.3",
    "dts-bundle-generator": "^7.2.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "lint-staged": "^13.1.1",
    "prettier": "^2.8.4",
    "stylelint": "^14.16.1",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "taze": "^0.8.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "unplugin-auto-import": "^0.14.2",
    "unplugin-vue-macros": "^1.7.3",
    "vite": "^4.1.1",
    "vite-plugin-pages": "^0.28.0",
    "vite-plugin-vue-layouts": "^0.7.0",
    "vitest": "^0.28.4",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  }
}
